[Unit]
Description=A distributed service mesh
Documentation=https://www.consul.io/docs/index.html
After=network.target

[Service]
ExecStart=/usr/local/bin/consul agent -data-dir=/var/consul/ -node=rabbitmq -datacenter=vagrant -bind '{{ GetInterfaceIP "enp0s8" }}' -join 192.168.42.10 -config-dir="/etc/consul.d"

[Install]
WantedBy=multi-user.target
